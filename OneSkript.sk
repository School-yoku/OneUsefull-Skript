command /noblock:
    Options:
        parkour_map_name: <text>
    trigger:
        permission: skript.admin
        on break:
            player is in {@parkour_map_name}
            player's gamemode is not creative
            cancel event 
        on place:
            player is in {@parkour_map_name}
            player's gamemode is not creative
            cancel event 
